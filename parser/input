load_primitive cube my_cube;

camera a {
    position: 0 0 0;
    direction: 0 0 1;
    depth: 100;
}

camera b {
	position: 0 0 0;
	direction: 0 0 1;
	depth: 100;
}

animation slide {
    interpolate 5 {
        translate my_cube 5 0 0;
		rotate my_cube 45 45 0;
		scale my_cube 5 5 5;
    }
}

animation slide_back {
	interpolate 5 {
		translate my_cube 0 0 0;
	}
}

add_to_timeline slide 4;
add_to_timeline slide_back 9;

set_default_camera a;
